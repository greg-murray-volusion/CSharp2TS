{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es7"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "noUnusedParameters": true,
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts"
    ],
    "exclude": [
        "node_modules/**/*.ts",
        ".vscode-test",
        "gifExample.ts"
    ]
}